{
  "require": {
    "clubstudioltd/craft-asset-rev": "6.0.2",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/cms": "3.4.13",
    "craftcms/redactor": "2.6.0.1",
    "superbig/craft3-mobiledetect": "1.0.2",
    "topshelfcraft/wordsmith": "3.1.2",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
